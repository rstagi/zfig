{
  "name": "@zfig/yaml-loader",
  "version": "0.9.15",
  "description": "YAML loader for zfig configuration library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "config",
    "zfig",
    "yaml",
    "loader"
  ],
  "author": "rstagi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstagi/zfig.git",
    "directory": "packages/yaml-loader"
  },
  "peerDependencies": {
    "zfig": "^0.9.15"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "zfig": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
