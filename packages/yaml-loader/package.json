{
  "name": "@confts/yaml-loader",
  "version": "0.9.1",
  "description": "YAML loader for confts configuration library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "config",
    "confts",
    "yaml",
    "loader"
  ],
  "author": "rstagi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstagi/confts.git",
    "directory": "packages/yaml-loader"
  },
  "peerDependencies": {
    "confts": "^0.9.1"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "confts": "workspace:*"
  }
}
