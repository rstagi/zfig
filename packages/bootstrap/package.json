{
  "name": "@zfig/bootstrap",
  "version": "0.9.14",
  "description": "Server bootstrap helper for zfig configuration library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "config",
    "zfig",
    "bootstrap",
    "server"
  ],
  "author": "rstagi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstagi/zfig.git",
    "directory": "packages/bootstrap"
  },
  "peerDependencies": {
    "zfig": "^0.9.14",
    "@zfig/yaml-loader": "^0.9.14"
  },
  "peerDependenciesMeta": {
    "@zfig/yaml-loader": {
      "optional": true
    }
  },
  "devDependencies": {
    "zfig": "workspace:*",
    "@zfig/yaml-loader": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
